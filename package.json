{
  "name": "yafuz",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "config/*",
    "lib/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel --filter=app-yafuz --filter=presale",
    "start": "turbo run start --parallel --filter=app-yafuz --filter=presale",
    "lint": "turbo run lint",
    "prettier-lint": "prettier --check .",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky install",
    "test": ""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && prettier --write . && git add -a ."
    }
  },
  "lint-staged": {
    "*.{ts}": [
      "turbo run lint"
    ]
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "turbo": "latest",
    "typescript": "^4.7.4"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "dependencies": {},
  "packageManager": "yarn@1.22.19"
}
